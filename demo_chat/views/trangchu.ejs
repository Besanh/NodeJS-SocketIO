<html>
    <head>
        <meta charset="utf-8">
        <title>Demo Chat</title>
        <script src="jquery.js"></script>
        <script src="socket.io/socket.io.js"></script>
        <script src="xuly.js"></script>
        <link rel="stylesheet" type="text/css" href="layout.css">
    </head>
    <body>
        <div id="wrapper">
            <div id="loginForm">
                <h3>What's your name ?</h3>
                <input type="text" id="txtName">
                <input type="button" id="btnRegister" value="Register">
            </div>
            <div id="chatForm">
                <div id="left">
                    <div id="boxTitle">User Online</div>
                    <div id="boxContent">
                        <div class="userOnline">Anh Le</div>
                        <div class="userOnline">Tien</div>
                        <div class="userOnline">Huan</div>
                    </div>
                </div>
                <div id="right">
                    <div id="sayHi">Hello <span id="curUser"></span>
                        <input type="button" id="btnLogout" value="Logout">
                    </div>
                    <div id="listMessage">
                        <input type="text" name="message" id="txtMessage">
                        <input type="button" value="Send" id="btnSend">
                    </div>
                </div>
            </div>
        </div>
    </body>
</html>